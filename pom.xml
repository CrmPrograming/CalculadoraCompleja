<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>dad.maven.calculadoracompleja</groupId>
  <artifactId>CalculadoraCompleja</artifactId>
  <version>0.0.1</version>
  
  <!-- DATOS BÁSICOS DEL PROYECTO -->
	<name>CalculadoraCompleja</name>
	<description>Práctica para la calculadora de números complejos.</description>

	<!-- PROPIEDADES DEL PROYECTO -->
	<properties>
		<maven.compiler.target>14</maven.compiler.target>
		<maven.compiler.source>14</maven.compiler.source>
		<exec.mainClass>dad.maven.calculadoracompleja.Main</exec.mainClass>
	</properties>

	<!-- DEPENDENCIAS DEL PROYECTO -->
	<dependencies>
		<!-- Librerías de JavaFX -->
		<dependency>
			<groupId>org.openjfx</groupId>
			<artifactId>javafx-controls</artifactId>
			<version>14.0.1</version>
		</dependency>
		<dependency>
			<groupId>org.openjfx</groupId>
			<artifactId>javafx-fxml</artifactId>
			<version>14.0.1</version>
		</dependency>
	</dependencies>
</project>